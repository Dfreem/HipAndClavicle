@model MerchantVM
@{
}
<table class="table table-bordered table-responsive-sm">
    <thead>
        <tr>
            <th>Name</th>
            <th>Order Date</th>
            
        </tr>
    </thead>
    <tbody>
        @foreach (Order order in Model.CurrentOrders)
        {
            <tr>

                @Html.DisplayFor(_ => order)

            </tr>
        }

    </tbody>
</table>